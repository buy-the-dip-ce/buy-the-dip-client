{
    "name": "buy-the-dip-client",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "vite",
        "dev": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "build:example:async": "rollup -c examples/async/rollup.config.js",
        "start:example:async": "node examples/async/lib/index.js",
        "build:example:ssg": "rollup -c examples/ssg/rollup.config.js && node --trace-warnings examples/ssg/export.js",
        "start:example:ssg": "npx serve examples/ssg/public -l 8080",
        "build:example:ssr": "rollup -c ssr/rollup.config.js",
        "start:example:ssr": "node ./ssr/dist/index.js",
        "build:example:stream": "rollup -c examples/stream/rollup.config.js",
        "start:example:stream": "node examples/stream/lib/index.js",
        "clean": "rimraf examples/**/lib/ examples/**/public/"
    },
    "repository": "https://github.com/buy-the-dip-ce/buy-the-dip-client.git",
    "author": "jerrynim <jerrynim@zigbang.com>",
    "license": "MIT",
    "packageManager": "yarn@3.2.0",
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-typescript": "^8.3.1",
        "@types/babel__core": "^7",
        "@types/express": "^4",
        "babel-preset-solid": "^1.3.6",
        "express": "^4.17.3",
        "rollup": "^2.68.0",
        "rollup-plugin-copy": "^3.4.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5",
        "vite": "^2.8.0",
        "vite-plugin-solid": "^2.2.5"
    },
    "dependencies": {
        "solid-app-router": "^0.3.0",
        "solid-js": "^1.3.7"
    }
}
